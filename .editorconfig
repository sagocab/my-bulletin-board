# top-most EditorConfig file
root = true

[*]
# [encoding-utf8]
charset = utf-8

# [newline-lf]
end_of_line = lf

# [newline-eof]
insert_final_newline = true

indent_style = space
# [4-spaces-tab]
indent_size = 2
tab_width = 2

# [no-trailing-spaces]
trim_trailing_whitespace = true

[line-length-120]
max_line_length = 120